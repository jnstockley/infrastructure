PAPERLESS_AUTO_LOGIN=true
PAPERLESS_AUTO_CREATE=true
PAPERLESS_LOGOUT_REDIRECT_URL=<authentik.host>/application/o/<slug>/end-session/

# NC Sync
WEBDRIVE_URL=$NEXTCLOUD_URL
