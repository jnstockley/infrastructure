USERMAP_UID=1000
USERMAP_GID=1000
PAPERLESS_URL=https://paperless.local.jstockley.com
PAPERLESS_SECRET_KEY=
PAPERLESS_TIME_ZONE=America/Chicago
PAPERLESS_OCR_LANGUAGE=eng
PAPERLESS_REDIS=<url>
PAPERLESS_DBENGINE=postgresql
PAPERLESS_DBHOST=<host>
